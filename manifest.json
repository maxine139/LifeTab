{
  "name": "LifeTab",
  "version": "1.0",
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "permissions": ["storage"],

  "chrome_url_overrides": {
    "newtab": "lifetab.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
